{
  "name": "ui",
  "version": "0.0.0",
  "type": "module",
  "author": {
    "name": "Milovan Gudelj",
    "email": "milovan.gudelj@gmail.com"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "main": "./components/index.tsx",
  "files": [
    "./dist/**/*",
    "./src/components/**/*"
  ],
  "exports": {
    ".": [
      "./src/components/index.tsx",
      "./dist/index.d.ts",
      "./dist/index.d.ts.map"
    ],
    "./styles.css": "./dist/index.css",
    "./globals.css": "./src/styles/globals.css"
  },
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build:types": "tsc",
    "build:css": "tailwindcss -i ./src/styles/globals.css -o ./dist/index.css --minify",
    "build": "pnpm clean && pnpm build:types && pnpm build:css",
    "lint": "eslint src/components/",
    "dev": "pnpm clean && concurrently \"pnpm build:types --watch\" \"pnpm build:css --watch\"",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "@tailwindcss/forms": "^0.5.6"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.6",
    "@turbo/gen": "latest",
    "@types/node": "^20.8.5",
    "@types/react": "^18.2.22",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.1",
    "eslint-config-iowt": "workspace:*",
    "iowt-tailwind-config": "workspace:*",
    "postcss": "^8.4.30",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.3.3",
    "tsconfig": "workspace:*",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.25.2",
    "@hookform/resolvers": "^3.3.1",
    "@phosphor-icons/react": "^2.0.13",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "icons": "workspace:*",
    "libphonenumber-js": "^1.10.45",
    "next": "^13.5.3",
    "react": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "zod": "^3.22.2"
  }
}